<script>

    var tablinks = document.getElementsByClassName("tab-links");
    var tabkontens = document.getElementsByClassName("tab-contens");
    
    function opentab(tabname){
        for(tablinks of tablink){
            tablink.classtlist.remove("active-link");
        }
        for(tabcontents of tabcotent){
            tabcontent.classtlist.remove("active-link");
        }
        event.currentTarget.classtlist.add("active-link");
    }

</script>

